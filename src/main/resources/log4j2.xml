<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <appenders>
    <ConsoleAppender name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </ConsoleAppender>
  </appenders>
  <loggers>
    <root level="INFO"/>
  </loggers>
</Configuration>